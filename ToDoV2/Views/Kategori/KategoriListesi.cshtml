@model IEnumerable<ToDoV2.Models.Kategori>

<div class="well">
    <h4>İş Kategorileri</h4>
    <div class="row">

        @foreach (var item in Model)
        {
            <div class="col-lg-6">
                <ul class="list-unstyled">
                    <li>
                        <a href="/Yapilacaklar/List/@item.Id">@item.KategoriAdi</a>
                    </li>
                    
                </ul>
            </div>
        }

    </div>
</div>
